<p id="notice"><%= notice %></p>

<h1>Careships</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @careships.each do |careship| %>
      <tr>
        <td><%= link_to 'Show', careship %></td>
        <td><%= link_to 'Edit', edit_careship_path(careship) %></td>
        <td><%= link_to 'Destroy', careship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Careship', new_careship_path %>
